       <property name="playTime" column="play_time" type="timestamp" />
       <property name="bcNum" column="bc_num" type="java.lang.Integer" />
       <property name="bcAmount" column="bc_amount" type="java.lang.Double" />
       <property name="preAmount" column="pre_amount" type="java.lang.Double" />
       <property name="successNum" column="success_num" type="java.lang.Integer" />
