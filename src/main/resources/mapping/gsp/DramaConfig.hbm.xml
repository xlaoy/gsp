<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
    <class name="com.gewara.model.drama.DramaConfig" table="drama_config" schema="gsp">
    
       <id name="recordid" type="long">
            <column name="recordid"  />
			<generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
			    <param name="optimizer">none</param>
			    <param name="increment_size">1</param>
			    <param name="sequence_name">seq_drama</param>
			</generator>
       </id>
       
       <property name="dramaid"		column="dramaid"	type="long" />
       <property name="suppliercode"		column="suppliercode"	type="string" />
       <property name="distype"		column="distype"	type="string" />
       <property name="dramaversionid"		column="dramaversionid"	type="long" />
       <property name="settlebase"		column="settlebase"	type="string" />
       <property name="settlecycle"		column="settlecycle"	type="string" />
       <property name="status"		column="status"	type="string" />
       <property name="addtime"		column="addtime"	type="timestamp" />
       <property name="starttime"		column="starttime"	type="timestamp" />
       <property name="endtime"		column="endtime"	type="timestamp" />
       <property name="gspupdatetime"		column="gspupdatetime"	type="timestamp" />
       <property name="lastendtime"		column="lastendtime"	type="timestamp" />
       <property name="lastjitiendtime"		column="lastjitiendtime"	type="timestamp" />
       <property name="canjiti"		column="canjiti"	type="string" />
        
    </class>
</hibernate-mapping>
