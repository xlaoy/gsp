<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
    <class name="com.gewara.model.drama.DramaOrderOffline" table="drama_orderoffline" schema="gsp">
    
       <id name="tradeno" type="string">
            <column name="tradeno"  />
            <generator class="assigned" />
       </id>
       
       <property name="status"		column="status"	type="string" />
       <property name="dramaid"		column="dramaid"	type="long" />
       <property name="paidtime"		column="paidtime"	type="timestamp" />
       <property name="ordertime"		column="ordertime"	type="timestamp" />
       <property name="updatetime"		column="updatetime"	type="timestamp" />
       <property name="username"		column="username"	type="string" />
       <property name="paymethod"		column="paymethod"	type="string" />
       <property name="otherfee"		column="otherfee"	type="double" />
       <property name="gspupdatetime"		column="gspupdatetime"	type="timestamp" />
      
        
    </class>
</hibernate-mapping>
