       <property name="refundAmount" column="refund_amount" type="java.lang.Double" />
       <property name="kpamount" column="kpamount" type="java.lang.Double" />
       <property name="goodsNum" column="goods_num" type="java.lang.Integer" />
       <property name="goodsAmount" column="goods_amount" type="java.lang.Double" /> 
       <property name="goodsSettleId" column="goods_settleid" type="long" />
