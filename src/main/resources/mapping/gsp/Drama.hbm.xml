<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
    <class name="com.gewara.model.drama.Drama" table="drama" schema="gsp">
    
       <id name="recordid" type="long">
            <column name="recordid"  />
            <generator class="assigned" />
       </id>
       
       <property name="dramaname"		column="dramaname"	type="string" />
       <property name="pretype"		column="pretype"	type="string" />
       <property name="origin"		column="origin"	type="string" />
       <property name="addtime"		column="addtime"	type="timestamp" />
       <property name="gspupdatetime"		column="gspupdatetime"	type="timestamp" />
        
    </class>
</hibernate-mapping>
